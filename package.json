{
  "name": "auth",
  "version": "0.6.2",
  "private": true,
  "scripts": {
    "start": "vercel dev",
    "start:all": "concurrently --parallel -n 'web,redis' 'yarn start:web' 'yarn start:redis'",
    "start:web": "react-app-rewired start",
    "start:redis": "docker run -p 6379:6379 redis",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "pull:dev": "vercel env pull --environment development",
    "pull:preview": "vercel env pull --environment preview",
    "pull:prod": "vercel env pull --environment production",
    "load": "ts-node ./scripts/create-schema.ts",
    "env": "env-cmd --no-override --file ${ENV_PATH:-.env}",
    "fix": "eslint \"./src/**/*.{ts,tsx}\" webpack.config.js --fix"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.1.0",
    "@chakra-ui/react": "^2.4.4",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/inter": "^4.5.14",
    "@polybase/client": "^0.6.2",
    "@polybase/eth": "^0.6.2",
    "@polybase/react": "^0.6.2",
    "@polybase/util": "^0.6.2",
    "@sentry/node": "^7.31.1",
    "@sentry/react": "^7.28.0",
    "@sentry/tracing": "^7.28.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "axios": "^1.2.3",
    "framer-motion": "^7.10.3",
    "history": "^5.3.0",
    "ioredis": "^5.2.5",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3",
    "penpal": "^6.2.2",
    "posthog-js": "^1.38.1",
    "postmark": "^3.0.15",
    "react": "^18.1.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.1.0",
    "react-ga": "^3.3.1",
    "react-hook-form": "^7.41.0",
    "react-icons": "^4.7.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^6.5.0",
    "react-scripts": "5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.4",
    "useragent": "^2.3.0",
    "web-vitals": "^3.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/prop-types": [
        2,
        {
          "skipUndeclared": true
        }
      ],
      "react/jsx-handler-names": "off",
      "quotes": "off",
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "react/display-name": 0,
      "semi": [
        "error",
        "never"
      ],
      "no-underscore-dangle": 0,
      "space-before-function-paren": 0,
      "arrow-body-style": 0,
      "no-use-before-define": 0,
      "arrow-parens": 0,
      "no-trailing-spaces": "error",
      "@typescript-eslint/quotes": [
        "error",
        "single"
      ],
      "@typescript-eslint/indent": [
        "error",
        2
      ]
    }
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "last 2 edge version"
    ]
  },
  "devDependencies": {
    "@metamask/providers": "^10.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.5.2",
    "@types/js-cookie": "^3.0.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/keccak": "^3.0.1",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/react-loadable": "^5.5.6",
    "@types/react-resizable": "^3.0.3",
    "@types/react-table": "^7.7.12",
    "@types/useragent": "^2.3.1",
    "@vercel/node": "^2.8.14",
    "assert": "^2.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.6.0",
    "crypto-browserify": "^3.12.0",
    "env-cmd": "^10.1.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.5.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-textarea-autosize": "^8.4.0",
    "source-map-explorer": "^2.5.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-node": "^10.9.1",
    "url": "^0.11.0",
    "vercel": "^28.12.8"
  }
}
